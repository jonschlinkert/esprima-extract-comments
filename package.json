    "esprima",
    "extract",
    "glob",
    "javascript",
    "parse"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "extract-comments",
        "js-comments",
        "parse-comments"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
